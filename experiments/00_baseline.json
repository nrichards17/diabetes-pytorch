{
  "name": "00_baseline",

  "dataset": "baseline",

  "batch_size": 256,

  "model": {
    "dropout_emb": 0.2,
    "dropout_fc": 0.6,
    "size_fc": [1024],
    "size_final": 16
  },

  "optimizer": {
    "type": "Adam",
    "weight_decay": 1.0e-6
  },

  "scheduler": {
    "type": "OneCycle",
    "max_epochs": 50,
    "eta_min": 1.0e-5,
    "eta_max": 1.0e-2,
    "epsilon": 0.0,
    "end_fraction": 0.1
  }
}